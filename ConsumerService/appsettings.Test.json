{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ConsumerService": "Debug",
      "Quartz": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=consumer_test.db",
    "Kafka": "localhost:9092"
  },
  "ProducerService": {
    "BaseUrl": "http://localhost:5299"
  },
  "ConsumerGroups": [
    {
      "GroupName": "test-consumer-group",
      "Topics": ["user-events", "order-events"]
    }
  ],
  "Agent": {
    "HeartbeatIntervalMs": 5000,
    "Name": "Consumer-Test-01"
  },
  "Consumer": {
    "ProcessingIntervalMs": 2000,
    "BatchSize": 5
  },
  "UseInMemoryKafka": true
}
