{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ProducerService": "Debug",
      "Quartz": "Information"
    }
  },
  "AllowedHosts": "*",
  "OutboxProcessor": {
    "ProcessingIntervalMs": 2000,
    "RetryIntervalMs": 10000,
    "BatchSize": 10,
    "RetryTimeoutMinutes": 5,
    "CleanupOlderThanDays": 1
  },
  "Agent": {
    "HeartbeatIntervalMs": 5000,
    "Name": "Producer-Test-01"
  },
  "UseInMemoryKafka": true
}
