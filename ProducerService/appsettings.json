{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ProducerService": "Debug"
    }
  },
  "AllowedHosts": "*",
  "OutboxProcessor": {
    "ProcessingIntervalMs": 5000,
    "RetryIntervalMs": 30000,
    "BatchSize": 100,
    "RetryTimeoutMinutes": 30,
    "MaxRetries": 3
  },
  "ConsumerGroups": [
    "default-consumer-group",
    "analytics-group",
    "notification-group"
  ]
}
